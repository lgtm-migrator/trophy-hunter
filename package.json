{
  "name": "trophy-hunter",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "client:dev": "vite",
    "server:dev": "ts-node-dev --project tsconfig.server.json src/server.ts",
    "client:build": "vite build",
    "server:build": "tsc --project tsconfig.server.json",
    "build": "npm run server:build && npm run client:build",
    "prepare": "husky install",
    "test": "tsc && tsc --project tsconfig.server.json --noEmit && prettier --check . && eslint .",
    "start": "node dist/server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lmachens/trophy-hunter.git"
  },
  "author": "Leon Machens",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lmachens/trophy-hunter/issues"
  },
  "homepage": "https://github.com/lmachens/trophy-hunter#readme",
  "dependencies": {
    "@emotion/css": "11.10.0",
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "ajv": "8.11.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "dotenv": "16.0.2",
    "express": "4.18.1",
    "helmet": "6.0.0",
    "isomorphic-unfetch": "3.1.0",
    "jsonwebtoken": "8.5.1",
    "lottie-react": "2.3.1",
    "markdown-to-jsx": "7.1.7",
    "mongodb": "4.9.1",
    "mongodb-cron": "1.8.0",
    "plausible-tracker": "0.3.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "3.39.2",
    "react-router-dom": "6.3.0"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/node": "18.7.15",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "5.36.2",
    "@vitejs/plugin-react": "2.1.0",
    "concurrently": "7.3.0",
    "eslint": "8.23.0",
    "eslint-plugin-react": "7.31.7",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "patch-package": "6.4.7",
    "prettier": "2.7.1",
    "ts-node-dev": "2.0.0",
    "typescript": "4.8.2",
    "vite": "3.1.0"
  }
}
