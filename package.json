{
  "name": "trophy-hunter",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "client:dev": "vite",
    "server:dev": "ts-node-dev --project tsconfig.server.json src/server.ts",
    "client:build": "vite build",
    "server:build": "tsc --project tsconfig.server.json",
    "build": "npm run server:build && npm run client:build",
    "prepare": "husky install",
    "test": "tsc && tsc --project tsconfig.server.json --noEmit && prettier --check . && eslint .",
    "start": "node dist/server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lmachens/trophy-hunter.git"
  },
  "author": "Leon Machens",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lmachens/trophy-hunter/issues"
  },
  "homepage": "https://github.com/lmachens/trophy-hunter#readme",
  "dependencies": {
    "@emotion/css": "^11.5.0",
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "ajv": "^8.8.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "isomorphic-unfetch": "^3.1.0",
    "jsonwebtoken": "^8.5.1",
    "markdown-to-jsx": "^7.1.5",
    "mongodb": "^4.2.1",
    "mongodb-cron": "^1.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-lottie": "^1.2.3",
    "react-query": "^3.34.0",
    "react-router-dom": "^6.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.11",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "@vitejs/plugin-react": "^1.1.0",
    "concurrently": "^6.4.0",
    "eslint": "^8.3.0",
    "eslint-plugin-react": "^7.27.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "patch-package": "^6.4.7",
    "prettier": "^2.5.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.2",
    "vite": "^2.6.14"
  }
}
